#: activity/activity.info:2
msgid "LocoSugar"
msgstr "LocoSugar"

#: game.py:37
msgid "Move the mouse to the Loco XO."
msgstr "Mova o mouse para o Loco XO."

#: game.py:38
msgid "Click on the Loco XO with the left button."
msgstr "Clique no Loco XO com o botão esquerdo."

#: game.py:39
msgid "Click on the Loco XOs with the left button."
msgstr "Clique em Loco XOs com o botão esquerdo."

#: game.py:40
msgid "Click and drag the Loco XOs to the right."
msgstr "Clique e arraste o Loco XOs para a direita."

# Would that be a letter as an specific character/symbol or as a message? 
#: game.py:41
msgid "Type the letter on the Loco XO."
msgstr "Digite a letra no Loco XO."

#: game.py:42
msgid "Use the SHIFT key for capital letters."
msgstr "Use a tecla SHIFT para letras maiúsculas."

#: game.py:43
msgid "Type the letters on the Loco XO in word order."
msgstr "Digite as letras no Loco XO na ordem correta."

#: game.py:44
msgid "Press ENTER to confirm."
msgstr "Pressione ENTER para confirmar."

#: game.py:45
msgid "Press DELETE to delete text."
msgstr "Pressione EXCLUIR para apagar o texto."

#: game.py:47
msgid "Hello LocoXO"
msgstr "Olá LocoXO"

#: game.py:47
msgid "LocoXOs are not real."
msgstr "LocoXOs não são reais."
