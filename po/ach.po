#: activity/activity.info:2
msgid "LocoSugar"
msgstr "LocoSugar"

#: game.py:37
msgid "Move the mouse to the Loco XO."
msgstr "Jer mouse i Loco XO."

#: game.py:38
msgid "Click on the Loco XO with the left button."
msgstr "Di kom Loco XO kede mapeca me tung cam."

#: game.py:39
msgid "Click on the Loco XOs with the left button."
msgstr "Di kom Loco XOs kede mapeca me tung cam."

#: game.py:40
msgid "Click and drag the Loco XOs to the right."
msgstr "Di ite coro Loco XOs tutung cem."

#: game.py:41
msgid "Type the letter on the Loco XO."
msgstr "Go nyukta ikom Loco XO."

#: game.py:42
msgid "Use the SHIFT key for capital letters."
msgstr "Ti kede kadiyo SHIFT pi nyukta adongodongo."

#: game.py:43
msgid "Type the letters on the Loco XO in word order."
msgstr "Go nyukta ikom Loco XO  iyoreyore me kop."

#: game.py:44
msgid "Press ENTER to confirm."
msgstr "Di ENTER me moko."

#: game.py:45
msgid "Press DELETE to delete text."
msgstr "Di DELETE me jwayo coc oco."

#: game.py:47
msgid "Hello LocoXO"
msgstr "Hello LocoXO"

#: game.py:47
msgid "LocoXOs are not real."
msgstr "LocoXOs pe tye ikome."
